RewriteEngine On


RewriteCond %{REQUEST_FILENAME} !-f


RewriteRule ^/?calendar/([A-Za-z0-9-_]+)/([A-Za-z0-9-_]+)$ calendar.php?y=$1&mm=$2

RewriteRule ^/?appointment/([A-Za-z0-9-_.*!?]+)/([A-Za-z0-9-_.*!?]+)$ appointment.php?a=$1&ai=$2
RewriteRule ^/?appointment/([A-Za-z0-9-_.*!?]+)$ appointment.php?a=$1

RewriteRule ^/?add/([A-Za-z0-9-_.*!?]+)$ add.php?date=$1


RewriteRule ^([^\.]+)$ $1.php [NC,L]